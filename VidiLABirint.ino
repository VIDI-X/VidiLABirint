void loop() {
  loadSave();
  mapToSeraial();
  if (analogRead(btn_lir) > 4000) {
    go_left();
    Serial.print("Move: W; ");
    measure();  //Measure distance from N, E, S & W
    delay(speed);
  }

  if (analogRead(btn_lir) > 1800 && analogRead(btn_lir) < 2200) {
    go_right();
    Serial.print("Move: E; ");
    measure();  //Measure distance from N, E, S & W
    delay(speed);
  }

  if (analogRead(btn_uid) > 4000) {
    go_up();
    Serial.print("Move: N; ");
    measure();  //Measure distance from N, E, S & W
    delay(speed);
  }

  if (analogRead(btn_uid) > 1800 && analogRead(btn_uid) < 2200) {
    go_down();
    Serial.print("Move: S; ");
    measure();  //Measure distance from N, E, S & W
    delay(speed);
  }

  drawWorld();
}
